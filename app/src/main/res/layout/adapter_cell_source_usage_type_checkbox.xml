<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/ACSUTC_tv_project"
        android:layout_width="500dp"
        android:layout_height="wrap_content"
        android:text="-project-"
        android:textSize="12sp"
        app:layout_constraintStart_toStartOf="@+id/ACSUTC_cb_main"
        app:layout_constraintTop_toBottomOf="@+id/ACSUTC_cb_main" />

    <View
        android:id="@+id/SUTC_v_margin"
        android:layout_width="wrap_content"
        android:layout_height="8dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ACSUTC_tv_project" />

    <CheckBox
        android:id="@+id/ACSUTC_cb_main"
        android:layout_width="40dp"
        android:layout_height="30dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/ACSUTC_tv_name"
        android:layout_width="500dp"
        android:layout_height="wrap_content"
        android:text="-name-"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/ACSUTC_cb_main"
        app:layout_constraintStart_toEndOf="@+id/ACSUTC_cb_main"
        app:layout_constraintTop_toTopOf="@+id/ACSUTC_cb_main" />
</androidx.constraintlayout.widget.ConstraintLayout>